{
	"name": "test",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "eslint . --cache",
		"format": "prettier --write --cache .",
		"hygiene:parallel": "run-p lint format i18n:analyze",
		"hygiene": "run-s check hygiene:parallel",
		"i18n:analyze": "node scripts/i18n/i18n-hygiene.mjs",
		"i18n:analyze-fix": "node scripts/i18n/i18n-hygiene.mjs --auto-fix",
		"i18n:check-missing": "node scripts/i18n/i18n-hygiene.mjs --check-missing",
		"i18n:validate-json": "node scripts/i18n/i18n-validate-json.mjs",
		"i18n:sort-keys": "node scripts/i18n/i18n-sort-keys.mjs",
		"i18n:remove-unused": "node scripts/i18n/i18n-remove-unused.mjs",
		"i18n:check-empty": "node scripts/i18n/i18n-check-empty.mjs",
		"i18n:auto-fix": "node scripts/i18n/i18n-auto-fix.mjs",
		"images:convert": "node scripts/convert-to-webp.mjs",
		"optimize-images": "node scripts/optimize-images.mjs",
		"generate-artwork-images": "node scripts/generate-artwork-images.mjs",
		"generate-artwork-data": "node scripts/generate-artwork-data.mjs",
		"generate-artwork-all": "npm run generate-artwork-images && npm run generate-artwork-data",
		"generate-sitemap": "node scripts/generate-sitemap.mjs"
	},
	"devDependencies": {
		"@eslint/compat": "^1.2.5",
		"@eslint/js": "^9.18.0",
		"@sveltejs/adapter-auto": "^6.0.0",
		"@sveltejs/adapter-vercel": "^5.10.2",
		"@sveltejs/kit": "^2.22.0",
		"@sveltejs/vite-plugin-svelte": "^6.0.0",
		"@tailwindcss/forms": "^0.5.9",
		"@tailwindcss/typography": "^0.5.15",
		"@tailwindcss/vite": "^4.0.0",
		"@types/node": "^24.5.2",
		"@zerodevx/svelte-img": "^2.1.2",
		"eslint": "^9.18.0",
		"eslint-config-prettier": "^10.0.1",
		"eslint-plugin-svelte": "^3.0.0",
		"globals": "^16.0.0",
		"npm-run-all": "^4.1.5",
		"prettier": "^3.4.2",
		"prettier-plugin-svelte": "^3.3.3",
		"svelte": "^5.0.0",
		"svelte-check": "^4.0.0",
		"tailwindcss": "^4.0.0",
		"tsx": "^4.19.2",
		"typescript": "^5.0.0",
		"typescript-eslint": "^8.20.0",
		"vite": "^7.0.4"
	},
	"dependencies": {
		"@vercel/analytics": "^1.5.0",
		"@vercel/speed-insights": "^1.2.0",
		"bigger-picture": "^1.1.19",
		"bits-ui": "^2.11.4",
		"fs": "^0.0.1-security",
		"lucide-svelte": "^0.544.0",
		"path": "^0.12.7",
		"sharp": "0.34.4",
		"svelte-i18n": "^4.0.1",
		"svelte-local-storage-store": "^0.6.4"
	}
}
